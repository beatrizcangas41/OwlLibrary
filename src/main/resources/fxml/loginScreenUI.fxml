<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" alignment="center" cache="true" cacheHint="QUALITY" hgap="5.0"
          vgap="5.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
          fx:controller="Controllers.loginScreenController">
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
    </rowConstraints>
    <columnConstraints>
        <ColumnConstraints minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints minWidth="10.0" prefWidth="100.0" />
    </columnConstraints>
    <TextField fx:id="usernameTextField" alignment="CENTER" prefHeight="26.0" prefWidth="393.0" GridPane.columnIndex="1"
               GridPane.columnSpan="4" GridPane.rowIndex="3">
        <GridPane.margin>
            <Insets left="50.0" />
        </GridPane.margin></TextField>
    <PasswordField fx:id="passwordTextField" GridPane.columnIndex="1" GridPane.columnSpan="4" GridPane.rowIndex="5">
        <GridPane.margin>
            <Insets left="50.0" />
        </GridPane.margin></PasswordField>


    <Button fx:id="loginButtonPressed" cacheHint="QUALITY" contentDisplay="CENTER" graphicTextGap="5.0"
            maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" onAction="#loginButtonPressed"
            prefWidth="155.0" text="Login" textAlignment="CENTER" textOverrun="CENTER_ELLIPSIS" GridPane.columnSpan="2"
            GridPane.rowIndex="7" GridPane.rowSpan="2">
        <GridPane.margin>
            <Insets bottom="15.0" left="50.0" top="15.0" />
        </GridPane.margin>
        <font>
            <Font name="Segoe UI" size="14.0" />
        </font>
        <cursor>
            <Cursor fx:constant="S_RESIZE" />
        </cursor>
    </Button>
    <Button fx:id="registerButtonPressed" cacheHint="QUALITY" contentDisplay="CENTER" graphicTextGap="5.0"
            maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" onAction="#loginButtonPressed"
            prefWidth="155.0" text="Register" textAlignment="CENTER" textOverrun="CENTER_ELLIPSIS"
            GridPane.columnIndex="4" GridPane.columnSpan="2" GridPane.rowIndex="7" GridPane.rowSpan="2">
        <font>
            <Font name="Segoe UI" size="14.0" />
        </font>
        <cursor>
            <Cursor fx:constant="S_RESIZE" />
        </cursor>
        <GridPane.margin>
            <Insets bottom="15.0" right="50.0" top="15.0" />
        </GridPane.margin>
    </Button>
    <cursor>
        <Cursor fx:constant="DEFAULT" />
    </cursor>
</GridPane>
